window = {
	name = "vicissitude_window"
	size = { 1076 850 }
	position = { 610 100 }
	layer = middle

	visible = "[And(Or(Not(IsObserver), GetPlayer.IsValid), GetPlayer.MakeScope.GetVariable('vicissitude_target').IsSet)]"
	datacontext = "[GetPlayer.MakeScope.GetVariable('vicissitude_target').GetCharacter]"

	using = Window_Background
	using = Window_Decoration
	using = Animation_ShowHide_Standard

	state = {
		name = _show
		using = Animation_FadeIn_Quick
		
		on_start = "[GetScriptedGui('vicissitude_window_start').Execute( GuiScope.SetRoot( GetPlayer.MakeScope.GetVariable('vicissitude_target').GetCharacter.MakeScope ).End)]"
	}

	state = {
		name = _hide
		using = Animation_FadeOut_Quick
	}
	
	vbox = {
		using = Window_Margins
		restrictparent_min = yes

		header_pattern = {
			layoutpolicy_horizontal = expanding

			blockoverride "header_text"
			{
				text = "VICISSITUDE_WINDOW_TITLE"
			}

			blockoverride "button_close"
			{
				onclick = "[GetScriptedGui('vicissitude_window_open').Execute( GuiScope.SetRoot( GetPlayer.MakeScope ).End)]"
			}
		}

		vbox = {
			layoutpolicy_horizontal = expanding
			layoutpolicy_vertical = expanding
			

			hbox = {
				name = "main"
				layoutpolicy_horizontal = expanding
				layoutpolicy_vertical = expanding

				## Left Side
				vbox = {
					name = "left"
					layoutpolicy_horizontal = expanding
					layoutpolicy_vertical = expanding
					vbox = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding
						margin = { 14 14 }

						background = {
							using = Background_Area
						}
						
						portrait_lifestyles = {
							size = { 430 850 }
						}
					}
				}

				## Right Side
