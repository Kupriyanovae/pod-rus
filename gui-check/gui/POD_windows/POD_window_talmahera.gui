window = {
	name = "talmahera_view"
	datacontext = "[GetVariableSystem]"
	datacontext = "[GetPlayer]"
	parentanchor = top|right
	allow_outside = yes
	movable = no
	layer = windows_layer
	
	visible = "[And(And(GetVariableSystem.HasValue('pod_sidebar', 'talmahera'), Not(IsRightWindowOpen)), IsSidebarVisibilityValid)]"
	datacontext = "[GetReligionByKey( 'talmahera_religion' )]"
	datacontext = "[GetPlayer.GetFaith]"

	using = Window_Size_MainTab

	state = {
		name = _show
		using = Animation_FadeIn_Quick
		using = Sound_WindowShow_Standard
		using = Window_Position_MainTab

		on_start = "[GetVariableSystem.Set('talmahera_tab_choice','edicts')]"
	}

	state = {
		name = _hide
		using = Animation_FadeOut_Quick
		using = Sound_WindowHide_Standard
		using = Window_Position_MainTab_Hide
	}

	margin_widget = {
		size = { 100% 100% }
		margin = { 0 26 }
		margin_right = 13

		widget = {
			size = { 100% 100% }

			### WINDOW CONTENT
			vbox = {
				using = Window_Margins
				ignoreinvisible = yes

				##### HEADER TEXT #####
				widget_header_with_picture = {
					layoutpolicy_horizontal = expanding
					minimumsize = { 0 210 }

					blockoverride "header_text" {
						text = "talmahera_religion"
					}

					blockoverride "size" {
						size = { 100% 228 }
					}

					blockoverride "illustration_texture" {
						texture = "gfx/interface/illustrations/window_headers/header_sects.dds"

						using = Mask_Rough_Edges

						modify_texture = {
							texture = "gfx/interface/component_masks/mask_fade_vertical.dds"
							blend_mode = alphamultiply
							mirror = vertical
						}
					}

					blockoverride "button_close"
					{
						onclick = "[GetVariableSystem.Clear('pod_sidebar')]"
						onclick = "[GetVariableSystem.Clear('talmahera_tab_choice')]"
					}

					vbox = {
						margin_top = 42
						margin_bottom = 10

						hbox = {
							spacing = 20

							text_single = {
								text = "TALMAHERA_WINDOW_TOTAL_INFLUENCE"
							}

							container = {
								text_single = {
									visible = "[GreaterThanOrEqualTo_CFixedPoint(EmptyScope.ScriptValue('talmahera_influence_value'), '(CFixedPoint)0')]"
									text = "TALMAHERA_WINDOW_INFLUENCE_GAIN"
									tooltip = "talmahera_influence_gain_tt"
								}

								text_single = {
									visible = "[LessThan_CFixedPoint(EmptyScope.ScriptValue('talmahera_influence_value'), '(CFixedPoint)0')]"
									text = "TALMAHERA_WINDOW_INFLUENCE_LOSE"
									tooltip = "talmahera_influence_gain_tt"
								}
							}
						}

						spacer = {
							layoutpolicy_vertical = expanding
						}

						text_label_center = {
							text = "TALMAHERA_WINDOW_MY_STATUS"
						}

						hbox = {
							layoutpolicy_horizontal = expanding
							margin_top = 12
							margin_left = 10
							margin_right = 10
							spacing = 15
							
							hbox = {
								vbox = {
									layoutpolicy_vertical = expanding
									spacing = 3

									hbox = {
										margin_left = 5
										layoutpolicy_horizontal = expanding

										text_label_left = {
											layoutpolicy_horizontal = expanding
											text = "TALMAHERA_WINDOW_ARALU"
											margin_left = 10
										}
									}

									hbox = {
										margin_left = 8

										background = {
											texture = "gfx/lines/line_lifestyles_unavailable.dds"
											margin = { -16 -12 }
											tintcolor = { 0.7 0.5 0.25 1 }
										}

										aralu_button = {
											datacontext = "[GetScriptedGui( 'talmahera_aralu_nergal' )]"
										}

										aralu_button = {
											datacontext = "[GetScriptedGui( 'talmahera_aralu_ninmug' )]"
											blockoverride "icon" { texture = "gfx/interface/icons/building_types/building_level_02.dds" }
										}

										aralu_button = {
											datacontext = "[GetScriptedGui( 'talmahera_aralu_loz' )]"
											blockoverride "icon" { texture = "gfx/interface/icons/building_types/building_level_03.dds" }
										}

										aralu_button = {
											datacontext = "[GetScriptedGui( 'talmahera_aralu_al_marhi' )]"
											blockoverride "icon" { texture = "gfx/interface/icons/building_types/building_level_04.dds" }
										}
									}
								}
							}

							vbox = {
								layoutpolicy_horizontal = expanding
								layoutpolicy_vertical = expanding
								datacontext = "[GetTitleByKey('e_talmahera').GetHolder]"
								
								text_label_left = {
									text = "delroh"
									layoutpolicy_horizontal = expanding
									margin_left = 2
								}

								expand = {}

								vbox = {
									layoutpolicy_horizontal = expanding

									text_single = {
										layoutpolicy_horizontal = expanding
										text = "[Character.GetUIName]"
										autoresize = no
										align = left
										max_width = 330
									}	

									vbox = {
										name = "info_text"
										layoutpolicy_horizontal = expanding
										layoutpolicy_vertical = expanding

										text_single = {
											visible = "[And( Character.IsAlive, Not( Character.IsImprisoned ) )]"
											layoutpolicy_horizontal = expanding
											text = "CHARACTER_LOCATION_TOOLTIP"
											align = nobaseline
										}

										text_single = {
											visible = "[And( Character.IsAlive, Character.IsImprisoned )]"
											layoutpolicy_horizontal = expanding
											text = "CHARACTER_IMPRISONED_TOOLTIP"
											align = nobaseline
										}

										expand = {}
									}
								}

								expand = {}
							}
						}

						expand = {}
					}

					widget = {
						allow_outside = yes
						size = { 89 98 }

						sect_symbol_big = {
							parentanchor = top|hcenter
							position = { 5 5 }
							blockoverride "symbol" { texture = "gfx/interface/icons/Symbol_Sect_TMR.dds" }
						}
					}
				}
				
				hbox = {
					name = "talmahera_tabs"
					layoutpolicy_horizontal = expanding

					button_tab = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding
						onclick = "[GetVariableSystem.Set('talmahera_tab_choice','wazir')]"
						down = "[GetVariableSystem.HasValue('talmahera_tab_choice', 'wazir')]"

						text = "TALMAHERA_WINDOW_TAB_WAZIR"
					}

					button_tab = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding
						onclick = "[GetVariableSystem.Set('talmahera_tab_choice','edicts')]"
						down = "[GetVariableSystem.HasValue('talmahera_tab_choice', 'edicts')]"

						text = "TALMAHERA_WINDOW_TAB_EDICTS"
					}

					button_tab = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding
						onclick = "[GetVariableSystem.Set('talmahera_tab_choice','hand')]"
						down = "[GetVariableSystem.HasValue('talmahera_tab_choice', 'hand')]"

						text = "TALMAHERA_WINDOW_TAB_HAND"
					}
				}
				
				expand = {}
				
				### WAZIR TAB
				vbox = {
					name = "tab_wazir"
					visible = "[GetVariableSystem.HasValue('talmahera_tab_choice', 'wazir')]"
					layoutpolicy_horizontal = expanding
					layoutpolicy_vertical = expanding

					using = Animation_Tab_Switch
					
					background = {
						name = "triangle_background"
						texture = "gfx/interface/illustrations/event_scenes/throneroom_west.dds"
						fittype = end
						alpha = 0.4
						framesize = { 1200 848 }

						using = Mask_Rough_Edges

						modify_texture = {
							texture = "gfx/interface/component_masks/mask_fade_vertical.dds"
							blend_mode = alphamultiply
							mirror = vertical
						}
					}
					
					scrollbox = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding

						blockoverride "scrollbox_margins" {
							margin = { 4 4 }
						}

						blockoverride "scrollbox_content"
						{
							fixedgridbox = {
								layoutpolicy_horizontal = expanding
								datamodel = "[Religion.MakeScope.GetList('wazir')]"
								addcolumn = 545
								addrow = 120
								setitemsizefromcell = yes

								item = {
									widget_sect_member_item = {
										datacontext = "[Scope.GetCharacter]"
										blockoverride "member_title" { text = "TALMAHERA_WINDOW_WAZIR_STATUS" }
									}
								}
							}
							expand = {}
						}
					}
				}
				
				### EDICTS TAB
				vbox = {
					name = "tab_edicts"
					visible = "[GetVariableSystem.HasValue('talmahera_tab_choice', 'edicts')]"
					layoutpolicy_horizontal = expanding
					layoutpolicy_vertical = expanding

					using = Animation_Tab_Switch
					
					background = {
						name = "triangle_background"
						texture = "gfx/interface/illustrations/event_scenes/throneroom_west.dds"
						fittype = end
						alpha = 0.4
						framesize = { 1200 848 }

						using = Mask_Rough_Edges

						modify_texture = {
							texture = "gfx/interface/component_masks/mask_fade_vertical.dds"
							blend_mode = alphamultiply
							mirror = vertical
						}
					}
					
					scrollbox = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding

						blockoverride "scrollbox_margins" {
							margin = { 4 4 }
						}

						blockoverride "scrollbox_content"
						{
							vbox = {
								spacing = 10
								margin = { 8 8 }

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_underworld_progress' )]"
									blockoverride "edict_title" { text = "talmahera_edict_underworld_progress" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_gain_fervor' )]"
									blockoverride "edict_title" { text = "talmahera_edict_gain_fervor" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_sabbat_building' )]"
									blockoverride "edict_title" { text = "talmahera_edict_sabbat_building" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_camarilla_building' )]"
									blockoverride "edict_title" { text = "talmahera_edict_camarilla_building" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_kj_building' )]"
									blockoverride "edict_title" { text = "talmahera_edict_kj_building" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_setite_corruption' )]"
									blockoverride "edict_title" { text = "talmahera_edict_setite_corruption" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_ashirra_edict_army' )]"
									blockoverride "edict_title" { text = "talmahera_edict_ashirra_edict_army" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}

								button_edict = {
									datacontext = "[GetScriptedGui( 'talmahera_edict_ashirra_edict_tax' )]"
									blockoverride "edict_title" { text = "talmahera_edict_ashirra_edict_tax" }
									blockoverride "texture" { texture = "gfx/interface/illustrations/decisions/decision_misc.dds" }
								}
							}

							expand = {}
						}
					}
				}
				
				### HAND TAB
				vbox = {
					name = "tab_hand"
					visible = "[GetVariableSystem.HasValue('talmahera_tab_choice', 'hand')]"
					layoutpolicy_horizontal = expanding
					layoutpolicy_vertical = expanding

					using = Animation_Tab_Switch
					
					background = {
						name = "triangle_background"
						texture = "gfx/interface/illustrations/event_scenes/throneroom_west.dds"
						fittype = end
						alpha = 0.4
						framesize = { 1200 848 }

						using = Mask_Rough_Edges

						modify_texture = {
							texture = "gfx/interface/component_masks/mask_fade_vertical.dds"
							blend_mode = alphamultiply
							mirror = vertical
						}
					}
					
					scrollbox = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding

						blockoverride "scrollbox_margins" {
							margin = { 4 4 }
						}

						blockoverride "scrollbox_content"
						{
							spacing = 10
							
							hand_entry_vbox = {
								visible = "[GetScriptedGui( 'camarilla_created' ).IsShown(GuiScope.SetRoot(GetPlayer.MakeScope).End)]"
								datacontext = "[GetReligionByKey( 'sabbat_religion' )]"
								blockoverride "wazir_modifiers" { tooltip = "TALMAHERA_WINDOW_HAND_SABBAT_WAZIR_MODIFIERS" }
							}
							hand_entry_vbox = {
								visible = "[GetScriptedGui( 'camarilla_created' ).IsShown(GuiScope.SetRoot(GetPlayer.MakeScope).End)]"
								datacontext = "[GetReligionByKey( 'camarilla_religion' )]"
								blockoverride "hand_color" { color = { 1 0.4 0.7 1 } }
								blockoverride "hand_icon" { texture = "gfx/interface/icons/Symbol_Sect_Cam.dds" }
								blockoverride "hand_type" { text = "TALMAHERA_WINDOW_HAND_CAMARILLA" }
								blockoverride "hand_host" { text = "camarilla_religion" }
								blockoverride "wazir_modifiers" { tooltip = "TALMAHERA_WINDOW_HAND_CAMARILLA_WAZIR_MODIFIERS" }
							}
							hand_entry_vbox = {
								datacontext = "[GetReligionByKey( 'ashirra_religion' )]"
								blockoverride "hand_color" { color = { 1 0.7 0.4 1 } }
								blockoverride "hand_icon" { texture = "gfx/interface/icons/Symbol_Sect_Ashr.dds" }
								blockoverride "hand_type" { text = "TALMAHERA_WINDOW_HAND_ASHIRRA" }
								blockoverride "hand_host" { text = "ashirra_religion" }
								blockoverride "wazir_modifiers" { tooltip = "TALMAHERA_WINDOW_HAND_ASHIRRA_WAZIR_MODIFIERS" }
							}
							hand_entry_vbox = {
								datacontext = "[GetReligionByKey( 'roadofset_religion' )]"
								blockoverride "hand_color" { color = { 0.0 0.7 0.0 1 } }
								blockoverride "hand_icon" { texture = "gfx/interface/icons/faith/viaset.dds" }
								blockoverride "hand_type" { text = "TALMAHERA_WINDOW_HAND_SETITE" }
								blockoverride "hand_host" { text = "roadofset_religion" }
								blockoverride "wazir_modifiers" { tooltip = "TALMAHERA_WINDOW_HAND_SETITE_WAZIR_MODIFIERS" }
							}
							hand_entry_vbox = {
								datacontext = "[GetReligionByKey( 'kueijin_religion' )]"
								blockoverride "hand_color" { color = { 1 1 1 1 } }
								blockoverride "hand_icon" { texture = "gfx/interface/icons/traits/kueijin.dds" }
								blockoverride "hand_type" { text = "TALMAHERA_WINDOW_HAND_KUEIJIN" }
								blockoverride "hand_host" { text = "trait_kueijin" }
								blockoverride "wazir_modifiers" { tooltip = "TALMAHERA_WINDOW_HAND_KUEIJIN_WAZIR_MODIFIERS" }
							}
							
							expand = {}
						}
					}
				}
			}
		}
	}
}

types Talmahera
{
    type aralu_button = icon_button_decorative 
	{
		size = { 50 50 }
		using = tooltip_below
		
		tooltip = "[ScriptedGui.BuildTooltip(GuiScope.SetRoot(Religion.MakeScope).AddScope('player',GetPlayer.MakeScope).End)]"
		enabled = "[ScriptedGui.IsValid(GuiScope.SetRoot(Religion.MakeScope).AddScope('player',GetPlayer.MakeScope).End)]"
		onclick = "[ScriptedGui.Execute(GuiScope.SetRoot(Religion.MakeScope).AddScope('player',GetPlayer.MakeScope).End)]"

		icon_button_decorative = {
			size = { 44 44 }
			parentanchor = center
			alwaystransparent = yes
		}
		
		icon_flat_standard_gold = {
			block "icon" { texture = "gfx/interface/icons/building_types/building_level_01.dds" }
			parentanchor = center
			size = { 32 32 }
			alwaystransparent = yes
			
			visible = "[Not(ScriptedGui.IsShown(GuiScope.SetRoot(Religion.MakeScope).AddScope('player',GetPlayer.MakeScope).End))]"
		}
		
		icon_flat_standard_gold = {
			block "icon" { texture = "gfx/interface/icons/building_types/building_level_01.dds" }
			parentanchor = center
			size = { 32 32 }
			alwaystransparent = yes
			
			visible = "[ScriptedGui.IsShown(GuiScope.SetRoot(Religion.MakeScope).AddScope('player',GetPlayer.MakeScope).End)]"
			using = Master_Button_Modify_Texture
			frame = 2
		}
	}
	
    type hand_entry_vbox = vbox_generic_object_card 
	{
		layoutpolicy_horizontal = expanding
		alwaystransparent = no

		blockoverride "header_properties"
		{
			layoutpolicy_horizontal = expanding
			margin = { 10 10 }
		}

		blockoverride "header_background_color"
		{
			modify_texture = {
				block "hand_color" { color = { 0.3 0.0 0.6 1 } }
				blend_mode = normal
			}
		}

		blockoverride "header_pre_contents"
		{
			icon = {
				block "hand_icon" { texture = "gfx/interface/icons/Symbol_Sect_Sab.dds" }
				size = { 50 50 }
			}
		}

		blockoverride "header_top_contents"
		{
			text_single = {
				block "hand_type" { text = "TALMAHERA_WINDOW_HAND_SABBAT" }
				default_format = "#high"
				max_width = 380

				using = Font_Size_Medium
				align = nobaseline

				parentanchor = vcenter|left
			}
		}

		blockoverride "header_bottom_contents"
		{
			text_single = {
				block "hand_host" { text = "sabbat_religion" }
				default_format = "#weak"

				using = Font_Size_Small
				align = nobaseline

				parentanchor = vcenter|left
			}
		}

		blockoverride "header_post_contents"
		{
			using = GenericObjectCardDefaultHeaderPostProperties

			text_single  = {
				text = "TALMAHERA_WINDOW_HAND_STATUS"
				default_format = "#weak"
				align = nobaseline
			}

			hbox = {
				spacing = -4

				icon_flat_standard_gold = {
					visible = "[GreaterThanOrEqualTo_CFixedPoint(Religion.MakeScope.ScriptValue('hand_status'), '(CFixedPoint)1')]"
					size = { 24 24 }
					texture = "gfx/interface/icons/flat_icons/pin.dds"
				}

				icon_flat_standard_gold = {
					visible = "[GreaterThanOrEqualTo_CFixedPoint(Religion.MakeScope.ScriptValue('hand_status'), '(CFixedPoint)2')]"
					size = { 24 24 }
					texture = "gfx/interface/icons/flat_icons/pin.dds"
				}

				icon_flat_standard_gold = {
					visible = "[GreaterThanOrEqualTo_CFixedPoint(Religion.MakeScope.ScriptValue('hand_status'), '(CFixedPoint)3')]"
					size = { 24 24 }
					texture = "gfx/interface/icons/flat_icons/pin.dds"
				}

				icon_flat_standard_black = {
					visible = "[Not(GreaterThanOrEqualTo_CFixedPoint(Religion.MakeScope.ScriptValue('hand_status'), '(CFixedPoint)1'))]"
					size = { 24 24 }
					texture = "gfx/interface/icons/flat_icons/pin.dds"

					glow = {
						glow_radius = 3
						using = Glow_Standard
						using = Color_White
						alpha = 0.08
					}
				}

				icon_flat_standard_black = {
					visible = "[Not(GreaterThanOrEqualTo_CFixedPoint(Religion.MakeScope.ScriptValue('hand_status'), '(CFixedPoint)2'))]"
					size = { 24 24 }
					texture = "gfx/interface/icons/flat_icons/pin.dds"

					glow = {
						glow_radius = 3
						using = Glow_Standard
						using = Color_White
						alpha = 0.08
					}
				}

				icon_flat_standard_black = {
					visible = "[Not(GreaterThanOrEqualTo_CFixedPoint(Religion.MakeScope.ScriptValue('hand_status'), '(CFixedPoint)3'))]"
					size = { 24 24 }
					texture = "gfx/interface/icons/flat_icons/pin.dds"

					glow = {
						glow_radius = 3
						using = Glow_Standard
						using = Color_White
						alpha = 0.08
					}
				}
			}
		}

		blockoverride "card_contents"
		{
			Background = {
				spriteType = Corneredtiled
				alpha = 0.15
				texture = "gfx/interface/colors/gold.dds"
				using = Mask_Rough_Edges

				modify_texture = {
					name = "mask"
					texture = "gfx/interface/component_masks/mask_fade_horizontal_middle_inverse.dds"
					spriteborder = { 0 0 }
					blend_mode = alphamultiply
				}

				modify_texture = {
					texture = "gfx/interface/component_masks/patterns/mask_pattern_floral.dds"
					spriteType = Corneredtiled
					blend_mode = alphamultiply
					alpha = 0.8
					texture_density = 2
				}

				modify_texture = {
					texture = "gfx/interface/colors/grey.dds"
					blend_mode = normal
				}
			}
			
			margin = { 10 0 }
			
			hbox = {
				layoutpolicy_horizontal = expanding
				layoutpolicy_vertical = expanding
				margin = { 4 10 }
				margin_bottom = 15
				spacing = 10

				vbox = {
					ignoreinvisible = yes
					text_single = {
						layoutpolicy_horizontal = expanding
						text = "TALMAHERA_WINDOW_HAND_TOTAL"
					}

					text_single = {
						layoutpolicy_horizontal = expanding
						visible = "[GreaterThanOrEqualTo_CFixedPoint(Religion.MakeScope.ScriptValue('hand_resistance_value'), '(CFixedPoint)0')]"
						text = "TALMAHERA_WINDOW_HAND_GAIN"
						tooltip = "hand_resistance_gain_tt"
					}

					text_single = {
						layoutpolicy_horizontal = expanding
						visible = "[LessThan_CFixedPoint(Religion.MakeScope.ScriptValue('hand_resistance_value'), '(CFixedPoint)0')]"
						text = "TALMAHERA_WINDOW_HAND_LOSE"
						tooltip = "hand_resistance_gain_tt"
					}

					text_single = {
						layoutpolicy_horizontal = expanding
						text = "TALMAHERA_WINDOW_HAND_ZEAL"
					}
				}

				expand = {}

				vbox = {
					text_single = {
						text = AFFECTING_MODIFIERS_LABEL
